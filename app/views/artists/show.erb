<h1><%= @artist.name %></h1>
<h3>Songs:</h3>
<ul>
  <% @songs.each do |song| %>
    <li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li>
  <% end %>
</ul>
<h3>Genres:</h3>
<ul>
  <% @genres.uniq.each do |genre| %>
    <li><a href="/genres/<%= genre.slug %>"><%= genre.name %></a></li>
  <% end %>
</ul>
