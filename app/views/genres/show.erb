<h1><%= @genre.name %></h1>
<h3>Songs:</h3>
<ul>
  <% @songs.each do |song| %>
    <li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li>
  <% end %>
</ul>
<h3>Artists:</h3>
<ul>
  <% @artists.uniq.each do |artist| %>
    <li><a href="/artists/<%= artist.slug %>"><%= artist.name %></a></li>
  <% end %>
</ul>
