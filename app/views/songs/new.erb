<form action="/songs" method="POST">
  <p>Song Name: <input id="Name" type="text" name="song[name]"></p>
  <p>Artist Name: <input id="Artist Name" type="text" name="artist[name]"></p>
  <p> Genres: <br> <br>
    <% Genre.all.each do |genre|%>
      <input type="checkbox" id="<%= genre.name %>" name="song[genre_ids][]" value="<%= genre.id %>" ><%= genre.name %></input>
    <% end %>
  </p>
  
  <br>
  <input type="submit" value="Create">
</form>